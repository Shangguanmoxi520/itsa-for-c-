#include <iostream>
using namespace std;

int main(int argc, char *argv[]) {
    int a, x0;
    cin >> a;
    x0 = a;
    int x2 = x0/100;
    x0 -= x2*100;
    int x1 = x0/10;
    x0 -= x1*10;
    if(a == x0*x0*x0 + x1*x1*x1 + x2*x2*x2) cout << "Yes" << endl;
    else cout << "No" << endl;
	return 0;
}
